  "main": "bot.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:local": "nodemon",
    "deploy": "tsc && pm2 restart ecosystem.config.js",
    "deploy:local": "tsc && pm2 restart ecosystem.config.js --env local",
    "deploy:docker": "tsc && pm2-runtime ecosystem.config.js --env local",
    "docker:build": "npm i && tsc && sudo docker build -t exclusivelogin/hellgame24-bot .",
    "docker:publish": "npm run docker:build && sudo docker login && sudo docker push exclusivelogin/hellgame24-bot",
    "docker:run": "sudo docker pull exclusivelogin/hellgame24-bot && sudo docker run --rm -d -p 8091:3000 exclusivelogin/hellgame24-bot"
  },
